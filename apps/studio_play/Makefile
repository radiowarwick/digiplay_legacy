####### Compile

.obj/main.o: main.cpp .ui/playoutform.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/main.o main.cpp

.ui/playoutform.h: playoutform.ui 
	$(UIC) playoutform.ui -o .ui/playoutform.h

.ui/playoutform.cpp: .ui/playoutform.h playoutform.ui 
	$(UIC) playoutform.ui -i playoutform.h -o .ui/playoutform.cpp

.obj/playoutform.o: .ui/playoutform.cpp .ui/playoutform.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/playoutform.o .ui/playoutform.cpp

.obj/moc_playoutform.o: .moc/moc_playoutform.cpp  .ui/playoutform.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_playoutform.o .moc/moc_playoutform.cpp

.moc/moc_playoutform.cpp: $(MOC) .ui/playoutform.h
	$(MOC) .ui/playoutform.h -o .moc/moc_playoutform.cpp

.obj/qmake_image_collection.o: .ui/qmake_image_collection.cpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/qmake_image_collection.o .ui/qmake_image_collection.cpp

.ui/qmake_image_collection.cpp: images/filenew \
