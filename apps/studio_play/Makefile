####### Compiler, tools and options

CC       = gcc
CXX      = g++
LEX      = flex
YACC     = yacc
CFLAGS   = -pipe -Wall -W -O2 -D_REENTRANT  -DQT_NO_DEBUG -DQT_THREAD_SUPPORT -DQT_SHARED -DQT_TABLET_SUPPORT
CXXFLAGS = -pipe -Wall -W -O2 -D_REENTRANT  -DQT_NO_DEBUG -DQT_THREAD_SUPPORT -DQT_SHARED -DQT_TABLET_SUPPORT
LEXFLAGS = 
YACCFLAGS= -d
INCPATH  = -I/usr/share/qt3/mkspecs/default -I. -I../../../../../include/qt3 -I.ui/ -I. -I.moc/
LINK     = g++
LFLAGS   = 
LIBS     = $(SUBLIBS) -L/usr/share/qt3/lib -L/usr/X11R6/lib -lqt-mt -lXext -lX11 -lm -lpthread
AR       = ar cqs
RANLIB   = 
MOC      = /usr/share/qt3/bin/moc
UIC      = /usr/share/qt3/bin/uic
QMAKE    = qmake
TAR      = tar -cf
GZIP     = gzip -9f
COPY     = cp -f
COPY_FILE= $(COPY)
COPY_DIR = $(COPY) -r
INSTALL_FILE= $(COPY_FILE)
INSTALL_DIR = $(COPY_DIR)
DEL_FILE = rm -f
SYMLINK  = ln -sf
DEL_DIR  = rmdir
MOVE     = mv -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p

####### Output directory

OBJECTS_DIR = .obj/

####### Files

HEADERS = 
SOURCES = main.cpp
OBJECTS = .obj/main.o \
		.obj/frmPlayout.o \
		.obj/qmake_image_collection.o
FORMS = frmPlayout.ui
UICDECLS = .ui/frmPlayout.h
UICIMPLS = .ui/frmPlayout.cpp
SRCMOC   = .moc/moc_frmPlayout.cpp
OBJMOC = .obj/moc_frmPlayout.o
DIST	   = studio_play.pro
QMAKE_TARGET = studio_play
DESTDIR  = 
TARGET   = studio_play

first: all
####### Implicit rules

.SUFFIXES: .c .o .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(OBJCOMP) $(LIBS)

mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: studio_play.pro  /usr/share/qt3/mkspecs/default/qmake.conf ../../../../../share/qt3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile studio_play.pro
qmake: 
	@$(QMAKE) -o Makefile studio_play.pro

dist: 
	@mkdir -p .obj/studio_play && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/studio_play/ && $(COPY_FILE) --parents images/fastbackward.png images/fastforward.png images/pause.png images/play.png images/reset.png images/stop.png .obj/studio_play/ && $(COPY_FILE) --parents frmPlayout.ui.h .obj/studio_play/ && ( cd `dirname .obj/studio_play` && $(TAR) studio_play.tar studio_play && $(GZIP) studio_play.tar ) && $(MOVE) `dirname .obj/studio_play`/studio_play.tar.gz . && $(DEL_FILE) -r .obj/studio_play

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:
	-$(DEL_FILE) $(UICIMPLS) $(UICDECLS)

yaccclean:
lexclean:
clean: mocclean uiclean
	-$(DEL_FILE) $(OBJECTS)
		-$(DEL_FILE) .ui/qmake_image_collection.cpp
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) $(TARGET) $(TARGET)


FORCE:

####### Compile

.obj/main.o: main.cpp .ui/frmPlayout.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/main.o main.cpp

.ui/frmPlayout.h: frmPlayout.ui 
	$(UIC) frmPlayout.ui -o .ui/frmPlayout.h

.ui/frmPlayout.cpp: .ui/frmPlayout.h frmPlayout.ui frmPlayout.ui.h 
	$(UIC) frmPlayout.ui -i frmPlayout.h -o .ui/frmPlayout.cpp

.obj/frmPlayout.o: .ui/frmPlayout.cpp frmPlayout.ui.h \
		.ui/frmPlayout.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/frmPlayout.o .ui/frmPlayout.cpp

.obj/moc_frmPlayout.o: .moc/moc_frmPlayout.cpp  .ui/frmPlayout.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_frmPlayout.o .moc/moc_frmPlayout.cpp

.moc/moc_frmPlayout.cpp: $(MOC) .ui/frmPlayout.h
	$(MOC) .ui/frmPlayout.h -o .moc/moc_frmPlayout.cpp

.obj/qmake_image_collection.o: .ui/qmake_image_collection.cpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/qmake_image_collection.o .ui/qmake_image_collection.cpp

.ui/qmake_image_collection.cpp: images/fastbackward.png \
		images/fastforward.png \
		images/pause.png \
		images/play.png \
		images/reset.png \
		images/stop.png
	$(UIC)  -embed studio_play images/fastbackward.png images/fastforward.png images/pause.png images/play.png images/reset.png images/stop.png -o .ui/qmake_image_collection.cpp

####### Install

install:  

uninstall:  

